#!/opt/ovirt-imageio/daemon/datamover/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cfn-lint==0.38.0','console_scripts','cfn-lint'
__requires__ = 'cfn-lint==0.38.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cfn-lint==0.38.0', 'console_scripts', 'cfn-lint')()
    )
